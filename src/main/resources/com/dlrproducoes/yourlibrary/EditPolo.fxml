<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dlrproducoes.yourlibrary.EditPolo">

   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="50.0">
         <children>
            <Label text="Editar polo">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="nome" promptText="Nome" />
            <TextField fx:id="endereco" layoutX="10.0" layoutY="10.0" promptText="Endereco">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </TextField>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button prefHeight="30.0" prefWidth="100.0" fx:id="buttonBack" onAction="#goBack" mnemonicParsing="false" text="Voltar" />
                  <Button prefHeight="30.0" prefWidth="100.0" fx:id="updateButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Editar">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </HBox>
         </children></VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
