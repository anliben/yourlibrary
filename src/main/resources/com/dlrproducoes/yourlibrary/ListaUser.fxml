<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dlrproducoes.yourlibrary.ListaUser">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Colaboradores">
        <font>
            <Font name="System Bold" size="14.0" />
        </font>
        <VBox.margin>
            <Insets bottom="20.0" />
        </VBox.margin>
    </Label>
    <TableView fx:id="tableView" editable="true" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true">
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
    </TableView>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" onAction="#goBack" text="Voltar">
                <HBox.margin>
                    <Insets right="20.0" />
                </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#goToNew" text="Criar" prefHeight="30.0" prefWidth="100.0" />
            <Button mnemonicParsing="false" onAction="#onHelloButtonClick" text="Continuar" prefHeight="30.0" prefWidth="100.0">
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin>
            </Button>
        </children>
    </HBox>
</VBox>
