<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="800.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dlrproducoes.yourlibrary.CreateUser">

    <children>
        <VBox alignment="CENTER" layoutX="50.0" layoutY="50.0">
            <children>
                <Label text="Criar Colaborador">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="nome" promptText="Nome" />
            <TextField fx:id="email" layoutX="10.0" layoutY="50.0" promptText="Email">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></TextField>
            <TextField fx:id="cpf" promptText="Cpf">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></TextField>
            <ChoiceBox id="choiceSetor" fx:id="choiceSetor" prefHeight="25.0" prefWidth="695.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></ChoiceBox>

                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="buttonBack" mnemonicParsing="false" onAction="#goBack" prefHeight="30.0" prefWidth="100.0" text="Voltar" />
                        <Button fx:id="buttonCreate" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="criar">
                            <HBox.margin>
                                <Insets left="15.0" />
                            </HBox.margin>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </HBox>
            </children>
         <padding>
            <Insets left="30.0" right="30.0" />
         </padding></VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
